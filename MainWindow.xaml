<Window x:Class="KhabirovaMasterPol.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:KhabirovaMasterPol"
        mc:Ignorable="d"
        Title="MasterPol" MinHeight="500" MinWidth="1000" WindowStartupLocation="CenterScreen" FontFamily="Segoe UI"  Icon ="pictures\Мастер пол.ico" >
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="auto"></RowDefinition>
            <RowDefinition Height="*"></RowDefinition>
        </Grid.RowDefinitions>
        <Frame Name="MainFrame" Source="MasterPage" Grid.Row="1"></Frame>
        <Image Source="pictures\Мастер пол.png" Height="70" HorizontalAlignment="Left"></Image>
        <TextBlock Text="Мастер Пол" FontSize="45" HorizontalAlignment="Center"></TextBlock>
        
    </Grid>
</Window>
